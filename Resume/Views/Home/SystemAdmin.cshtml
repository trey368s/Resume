@{
    ViewData["Title"] = "Systems Administration";
}
<h1>@ViewData["Title"]</h1>
<div class="row row-cols-1 row-cols-md-3 g-4">
    <div class="col">
        <div class="card bg-primary mb-3" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">Windows</h5>
                <ul class="list-group list-group-flush">
                    <li class="list-group-item list-group-item-action list-group-item-primary">Windows Server</li>
                    <li class="list-group-item list-group-item-action list-group-item-primary">Active Directory</li>
                    <li class="list-group-item list-group-item-action list-group-item-primary">IIS</li>
                    <li class="list-group-item list-group-item-action list-group-item-primary">DNS</li>
                    <li class="list-group-item list-group-item-action list-group-item-primary">Powershell</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="col">
        <div class="card bg-warning mb-3" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">Linux</h5>
                <ul class="list-group list-group-flush">
                    <li class="list-group-item list-group-item-action list-group-item-warning">Linux Terminal</li>
                    <li class="list-group-item list-group-item-action list-group-item-warning">PuTTY</li>
                    <li class="list-group-item list-group-item-action list-group-item-warning">WinSCP</li>
                    <li class="list-group-item list-group-item-action list-group-item-warning">Docker</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="col">
        <div class="card bg-success mb-3" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">Virtualization</h5>
                <ul class="list-group list-group-flush">
                    <li class="list-group-item list-group-item-action list-group-item-success">ESXi</li>
                    <li class="list-group-item list-group-item-action list-group-item-success">Hyper-V</li>
                    <li class="list-group-item list-group-item-action list-group-item-success">Proxmox</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="col">
        <div class="card bg-info mb-3" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">Administration In</h5>
                <ul class="list-group list-group-flush">
                   <li class="list-group-item list-group-item-action list-group-item-info">Adobe Creative Cloud</li>
                    <li class="list-group-item list-group-item-action list-group-item-info">Microsoft 365</li>
                    <li class="list-group-item list-group-item-action list-group-item-info">Autodesk</li>
                    <li class="list-group-item list-group-item-action list-group-item-info">Solidworks</li>
                    <li class="list-group-item list-group-item-action list-group-item-info">Connectwise Control</li>   
                    <li class="list-group-item list-group-item-action list-group-item-info">Bomgar BeyondTrust</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="col">
        <div class="card bg-danger mb-3" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">Programming Frameworks</h5>
                <ul class="list-group list-group-flush">
                    <li class="list-group-item list-group-item-action list-group-item-danger">Node.js</li>
                    <li class="list-group-item list-group-item-action list-group-item-danger">npm</li>
                    <li class="list-group-item list-group-item-action list-group-item-danger">ASP.NET</li>
                    <li class="list-group-item list-group-item-action list-group-item-danger">NuGet</li>
                    <li class="list-group-item list-group-item-action list-group-item-danger">Anaconda</li>
                    <li class="list-group-item list-group-item-action list-group-item-danger">pip</li>
                    <li class="list-group-item list-group-item-action list-group-item-danger">Thymeleaf</li>
                    <li class="list-group-item list-group-item-action list-group-item-danger">Spring Boot</li>
                    <li class="list-group-item list-group-item-action list-group-item-danger">Bootstrap</li>
                    <li class="list-group-item list-group-item-action list-group-item-danger">Jetpack Compose</li>   
                </ul>
            </div>
        </div>
    </div>
    <div class="col">
        <div class="card bg-secondary mb-3" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">Project Management</h5>
                <ul class="list-group list-group-flush">
                    <li class="list-group-item list-group-item-action list-group-item-secondary">Git</li>
                    <li class="list-group-item list-group-item-action list-group-item-secondary">Gradle</li>
                    <li class="list-group-item list-group-item-action list-group-item-secondary">Maven</li>
                    <li class="list-group-item list-group-item-action list-group-item-secondary">Jira</li>
                    <li class="list-group-item list-group-item-action list-group-item-secondary">Jitbit</li>
                    <li class="list-group-item list-group-item-action list-group-item-secondary">CI/CD</li>
                    <li class="list-group-item list-group-item-action list-group-item-secondary">Agile Methodology</li>
                </ul>
            </div>
        </div>
    </div>
</div>